  "objectID": "{{ $page.UniqueID }}",
  "href": "{{ $page.RelPermalink }}",
  "title": "{{ $page.Title }}",
  "tags": [{{ range $tindex, $tag := $page.Params.tags }}{{ if $tindex }}, {{ end }}"{{ $tag }}"{{ end }}],
  "content": "{{ replace ($page.Content | plainify | htmlUnescape | replaceRE "[[:space:]]" " ") "\"" " " }}",
  "image": "{{ .Params.background }}",
  "date": "{{ .Date.Format "2006-01-02T15:04:05Z07:00" }}"
},{{ end }}{{ end }}{{ end }}{{ end }}{{ end }}
